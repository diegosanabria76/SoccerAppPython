class team:

    def __init__(self, team_name, num_goals, points):
        self.team_name = team_name
        self.num_goals = num_goals
        self.points = points


    def print(self):
        print(self.team_name,self.num_goals,self.points)

    def add_result(self,my_team):
        self.num_goals = self.num_goals + my_team.num_goals
        self.points = self.points + my_team.points

    def print(self):
        print('\n Team Name: ', self.team_name, ' Goals: ', self.num_goals, ' Points: ', self.points)
